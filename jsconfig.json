{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "allowJs": true,
        "checkJs": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "skipLibCheck": true,
        "baseUrl": "src"
    },
    "include": ["src/**/*"]
}